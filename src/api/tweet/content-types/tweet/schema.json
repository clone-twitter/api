{
  "kind": "collectionType",
  "collectionName": "tweets",
  "info": {
    "singularName": "tweet",
    "pluralName": "tweets",
    "displayName": "Tweet",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "text_content": {
      "type": "text",
      "required": true
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "created_date": {
      "type": "datetime"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "tweet"
    },
    "retweets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::retweet.retweet",
      "mappedBy": "tweet"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "tweet"
    }
  }
}
